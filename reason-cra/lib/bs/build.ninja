g_pkg_flg = -bs-package-name reason-cra -bs-ns ReasonCra
src_root_dir = /Users/hick/Github/work/reason-playground/reason-cra
bsc = /Users/hick/Github/work/reason-playground/reason-cra/node_modules/bs-platform/darwin/bsc.exe
bsdep = /Users/hick/Github/work/reason-playground/reason-cra/node_modules/bs-platform/darwin/bsb_helper.exe
warnings = 
bsc_flags = -bs-super-errors
ppx_flags = 
g_dpkg_incls = 
g_ns = -bs-ns ReasonCra
g_lib_incls = -I . -I src/reason -I src -I /Users/hick/Github/work/reason-playground/reason-cra/node_modules/reason-react/lib/ocaml
rule build_ast_from_re
  command = $bsc  $warnings -bs-jsx 3 $bsc_flags -o $out -bs-syntax-only -bs-binary-ast $in
  description = [34mBuilding[39m [2m${out}[22m
build  src/reason/Button.reast : build_ast_from_re $src_root_dir/src/reason/Button.re
rule mk_deps
  command = $bsdep -hash 4fa5f0e7e0bfea45a22a2b870881d425 $g_ns $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/reason/Button.d : mk_deps src/reason/Button.reast
rule ml_cmj_cmi
  command = $bsc $g_pkg_flg -bs-suffix $g_lib_incls $warnings $bsc_flags -o $out $in
  dyndep = $in_e.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/reason/Button-ReasonCra.cmj |  src/reason/Button-ReasonCra.cmi $src_root_dir/src/reason/Button.bs.js : ml_cmj_cmi src/reason/Button.reast ||  src/reason/Button.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output es6:src/reason
rule build_package
  command = $bsc -w -49 -color always -no-alias-deps  $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  ReasonCra.cmi : build_package ReasonCra.mlmap
